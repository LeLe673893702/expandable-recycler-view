<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.newler.expandablerecyclerview.sample">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">

    <activity android:name="com.newler.expandablerecyclerview.sample.MainActivity">

      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity
      android:name="com.newler.expandablerecyclerview.sample.expand.ExpandActivity"
      android:parentActivityName="com.newler.expandablerecyclerview.sample.MainActivity" />

    <activity
      android:name="com.newler.expandablerecyclerview.sample.multicheck.MultiCheckActivity"
      android:parentActivityName="com.newler.expandablerecyclerview.sample.MainActivity" />

    <activity
      android:name="com.newler.expandablerecyclerview.sample.singlecheck.SingleCheckActivity"
      android:parentActivityName="com.newler.expandablerecyclerview.sample.MainActivity" />

    <activity
      android:name="com.newler.expandablerecyclerview.sample.multitype.MultiTypeActivity"
      android:parentActivityName="com.newler.expandablerecyclerview.sample.MainActivity" />

    <activity
      android:name="com.newler.expandablerecyclerview.sample.multitypeandcheck.MultiTypeCheckGenreActivity"
      android:parentActivityName="com.newler.expandablerecyclerview.sample.MainActivity" />

  </application>

</manifest>
